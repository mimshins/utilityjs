    ["@utilityjs/graph", "@utilityjs/queue", "@utilityjs/stack"]
  ],
  "access": "public",
  "baseBranch": "main",
  "bumpVersionsWithWorkspaceProtocolOnly": true,
  "prettier": false,
  "updateInternalDependencies": "patch",
  "ignore": [],
  "privatePackages": {
    "tag": false,
    "version": false
  }
